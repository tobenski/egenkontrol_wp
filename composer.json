{
    "type": "project",
    "config": {
        "vendor-dir": "content/vendor",
        "allow-plugins": {
            "johnpbloch/wordpress-core-installer": true,
            "composer/installers": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    },
    "require": {
        "johnpbloch/wordpress": ">=6.1",
        "wpackagist-plugin/advanced-custom-fields": "^6.0",
        "wpackagist-plugin/custom-post-type-ui": "^1.13",
        "wpackagist-plugin/wp-dummy-content-generator": "^2.1",
        "imranhsayed/headless-cms": "^1.7",
        "wp-graphql/wp-graphql": "^1.13",
        "wp-graphql/wp-graphql-jwt-authentication": "^0.6.0",
        "pristas-peter/wp-graphql-gutenberg": "^0.4.1",
        "yoast/wordpress-seo": "^20.0",
        "ashhitch/wp-graphql-yoast-seo": "^4.21"
    }
}
